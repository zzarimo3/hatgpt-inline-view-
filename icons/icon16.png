<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 16 16">
  <!-- Background -->
  <rect width="16" height="16" rx="3" fill="#10a37f" />
  
  <!-- Chat bubble left (question) -->
  <rect x="2" y="4" width="5" height="3" rx="1" fill="white" />
  
  <!-- Chat bubble right (answer) -->
  <rect x="9" y="4" width="5" height="3" rx="1" fill="#e7f7f4" />
  
  <!-- Inline connection line -->
  <line x1="7" y1="5.5" x2="9" y2="5.5" stroke="white" stroke-width="0.7" />
  
  <!-- Second row of bubbles -->
  <rect x="2" y="9" width="5" height="3" rx="1" fill="white" />
  <rect x="9" y="9" width="5" height="3" rx="1" fill="#e7f7f4" />
  <line x1="7" y1="10.5" x2="9" y2="10.5" stroke="white" stroke-width="0.7" />
</svg>
